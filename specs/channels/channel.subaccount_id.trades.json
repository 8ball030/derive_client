        },
        "params": {
          "type": "object",
          "$ref": "#/definitions/SubaccountIdTradesNotificationParamsSchema",
          "field_many": false
        }
      },
      "additionalProperties": false
    },
    "SubaccountIdTradesNotificationParamsSchema": {
